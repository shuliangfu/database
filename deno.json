{
  "name": "@dreamer/database",
  "version": "1.0.7",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.json",
      "LICENSE",
      "NOTICE",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/i18n": "jsr:@dreamer/i18n@^1.0.1",
    "@dreamer/service": "jsr:@dreamer/service@^1.0.2",
    "@dreamer/cache": "jsr:@dreamer/cache@^1.0.3",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.2",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.17",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.15",

    "mongodb": "npm:mongodb@7.0.0",
    "pg": "npm:pg@8.16.3",
    "mysql2": "npm:mysql2@3.16.0"
  },
  "nodeModulesDir": "auto"
}
