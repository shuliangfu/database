{
  "name": "@dreamer/database",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/cache": "jsr:@dreamer/cache@^1.0.0-beta.3",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0-beta.2",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@1.0.0-beta.16",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.12",
    "mongodb": "npm:mongodb@^7.0.0",
    "pg": "npm:pg@^8.16.3",
    "mysql2": "npm:mysql2@^3.16.0",
    "better-sqlite3": "npm:better-sqlite3@^11.0.0"
  },
  "nodeModulesDir": "auto",
  "allowScripts": {
    "allow": [],
    "deny": [
      "npm:better-sqlite3@11.10.0"
    ]
  }
}
