{
  "name": "@dreamer/database",
  "version": "1.0.4",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.json",
      "LICENSE",
      "NOTICE",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/i18n": "jsr:@dreamer/i18n@^1.0.1",
    "@dreamer/service": "jsr:@dreamer/service@^1.0.0",
    "@dreamer/cache": "jsr:@dreamer/cache@^1.0.0",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.8",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.7",
    "mongodb": "npm:mongodb@7.0.0",
    "pg": "npm:pg@8.16.3",
    "mysql2": "npm:mysql2@3.16.0",
    "better-sqlite3": "npm:better-sqlite3@11.0.0"
  },
  "nodeModulesDir": "auto",
  "allowScripts": {
    "allow": [],
    "deny": [
      "npm:better-sqlite3@11.0.0",
      "npm:better-sqlite3@11.10.0",
      "npm:esbuild@0.27.2",
      "npm:puppeteer@24.36.1"
    ]
  }
}
